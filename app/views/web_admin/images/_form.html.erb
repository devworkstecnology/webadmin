<div class="row">
  <div class="col-md-12">
    <div class="panel">
      <div class="panel-body">
        <div class="row images">
          <%= render 'web_admin/images/images', album: @album %>
        </div>
        <script id="template-upload" type="text/x-tmpl">
          <div class="upload">
            {%=o.name%}
            <div class="progress"><div class="bar" style="width: 0%; background-color: green;">&nbsp;</div></div>
          </div>
        </script>

        <%= simple_form_for WebAdmin::Image.new, html: { class: "form-horizontal", role: "form", multipart: true} do |f| %>
          <%= f.input :album_id, as: :hidden, input_html: { value: @album.id } %>
          <%= f.input :file, as: :file, label: false, :input_html => { class: "filestyle", multiple: true } %>
        <% end %>
      </div>
    </div>

  </div>
</div>
